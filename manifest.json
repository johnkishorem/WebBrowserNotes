{
  // Required fields of Chrome Extension Manifest
  "manifest_version": 2,
  "name": "WebBrowserNotes",
  "version": "0.1",

  //Fields for toolbar icon 
   "browser_action": {
   	"default_title" : "Page notes",
   	"default_popup" : "popup.html"
   },

   //Fields for Event Page
   "background" : {
   	"scripts" : ["js/event.js"],
   	"persistence" : false
   },

   "permissions" : [
   	"contextMenus",
   	"tabs",
    "storage"
   ]
  
}